<?xml version="1.0" encoding="utf-8"?>
<resources>
<!--    Se teclea y se busca el componenete-->
    <style name="RedEditText"  parent="Widget.AppCompat.TextView">
        <item name="android:textColor">@color/red</item>
        <item name="android:textSize">24sp</item>
        <item name="fontFamily">@font/bernier_shade_regular</item>
    </style>

    <style name="editTextGeneralPF"  parent="Widget.AppCompat.EditText">
        <item name="android:layout_width">250dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/green_monster</item>
        <item name="android:textColorHint">@color/black</item>
        <item name="android:fontFamily">@font/robotoslab_variablefont_wght</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="buttonGeneralPF"  parent="Widget.AppCompat.Button">
        <item name="android:layout_width">250dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textColorHint">@color/white</item>
        <item name="android:fontFamily">@font/robotoslab_variablefont_wght</item>
    </style>

    <style name="gnrl_headers"  parent="Widget.AppCompat.TextView">
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/pottaone_regular</item>
    </style>

    <style name="moveNextBtn"  >
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/links_blue</item>
        <item name="android:backgroundTint">@color/green_monster</item>
    </style>

    <style name="customImageViewRounded" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">32dp</item>
    </style>

</resources>